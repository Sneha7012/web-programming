<body>
<h1>HTML DOM Events</h1>



   <img src="c1.jpeg" alt="Image 1" id="image" class="image" onmouseover="changeToSecondImage()" onmouseout="changeToThirdImage()">

<script>

const img = document.getElementById('image');

function changeToSecondImage() {
    img.src = 'c2.jpeg';  
}

function changeToThirdImage() {
    img.src = 'c3.png';  
}

</script>

</body>
</html>

